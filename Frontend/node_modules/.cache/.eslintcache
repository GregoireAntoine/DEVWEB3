"/confirm\" component={Confirm}/>\r\n          <Route path=\"/acceuil\" component={Acceuil}/>\r\n          \r\n          </UserProfileContextProvider>\r\n       </Router>\r\n    </Fragment>\r\n  );\r\n}\r\nexport default App;","C:\\Users\\Grégoire Antoine\\Documents\\GitHub\\DEVWEB3\\frontend\\src\\app\\lib\\reducers.js",["72"],"import { actions } from './actions'\r\n\r\n\r\nconst saveToLocalStorage = object => {\r\n    localStorage.setItem(\"items\", JSON.stringify(object))\r\n}\r\nconst initialState = {\r\n    items : JSON.parse(localStorage.getItem(\"items\")) !== null ? JSON.parse(localStorage.getItem(\"items\")) : []\r\n}\r\n\r\n///////////////////////////////////Gestionnaire d'etat///////////////////////////////////////////////////////////\r\nexport default function onlineStoreApp(state = initialState, action) {\r\n    switch(action.type) {\r\n        case actions.ADD_TO_CART : return Object.assign({}, state, { items : [...state.items, action.payload]});\r\n        case actions.UPDATE_CART : \r\n        console.log(`item : ${action.payload.id}`)\r\n        return Object.assign({}, state, {\r\n            items : state.items.map(item => {\r\n                return item.id === action.payload.id ?\r\n                Object.assign({}, item, {\r\n                    quantity: action.payload.quantity\r\n                }) : item;\r\n            }) \r\n        })\r\n        case actions.REMOVE_FROM_CART : \r\n        console.log(state.items)\r\n        return Object.assign({}, state,  {\r\n            items : state.items.filter(item => {\r\n                return item.id != action.payload\r\n            })\r\n        })\r\n        case actions.SAVE_CART: \r\n        saveToLocalStorage(action.payload.items)\r\n        return state\r\n        case actions.RESET_CART:\r\n        saveToLocalStorage([])\r\n        return Object.assign({}, state,  {\r\n            items : []\r\n        })\r\n        default: return state\r\n    }\r\n}   ","C:\\Users\\Grégoire Antoine\\Documents\\GitHub\\DEVWEB3\\frontend\\src\\app\\lib\\UserProfileContext.js",[],"C:\\Users\\Grégoire Antoine\\Documents\\GitHub\\DEVWEB3\\frontend\\src\\app\\views\\components\\Home.js",["73"],"\r\nimport React from 'react'\r\nimport { List } from  '../../components'\r\n\r\nimport \"../../styles/App.css\";\r\nimport \"../../styles/Confirm.css\";\r\n\r\nconst SideMenu = ({ loadCategory, category }) => {\r\n    const links = [\"Pains\", \"Viennoiseries\", \"Pâtisseries\", \"Sandwicherie\"]\r\n    return(\r\n      <div className=\"col-sm-2 sidebar\">\r\n        <ul>\r\n          {links.map((link, index) => {\r\n            return(<li className={category == index && 'active'} key={index} onClick={() => loadCategory(index)}>{link}</li>)\r\n          })}\r\n        </ul>\r\n      </div>\r\n    \r\n    )\r\n  }\r\n\r\nexport const Home = props => {\r\n    const { isFiltering, filtered, list, category, loadCategory } = props\r\n    return( <div className=\"container\">\r\n    <div className=\"row\">\r\n      <SideMenu loadCategory={loadCategory} category={category}/>\r\n      <div className=\"col-sm\">\r\n        <div className=\"row\">\r\n           <List data={isFiltering ? filtered : list[category]} category={category} />\r\n        </div> \r\n      </div>\r\n    </div>\r\n  </div>)\r\n}\r\n","C:\\Users\\Grégoire Antoine\\Documents\\GitHub\\DEVWEB3\\frontend\\src\\app\\views\\components\\Cart.js",["74","75"],"import React, { Fragment, useEffect, useState } from 'react';\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport { Link } from \"react-router-dom\";\r\nimport \"../../styles/App.css\";\r\nimport { updateCart, removeFromCart } from  '../../lib/actions'\r\n\r\nconst Row = (props) => {\r\n    const { id, quantity, details } = props.item\r\n    const item = details\r\n    const [qty, setQty] = useState(quantity);\r\n    const dispatch = useDispatch()\r\n    const update = (action) => {\r\n      if (action === 'increment') { setQty(qty + 1) }\r\n      if (action === 'decrement') { setQty(qty - 1) }\r\n    }\r\n    const remove = id => {\r\n      dispatch(removeFromCart(id))\r\n    }\r\n    useEffect(() => {\r\n      dispatch(updateCart(id, qty))\r\n    }, [qty])\r\n    return (\r\n      <tr>\r\n        <td>\r\n          <img\r\n            width=\"70\"\r\n            height=\"70\"\r\n            src={process.env.PUBLIC_URL + `/assets/${item.category}/${item.image}`}\r\n            alt={item.name}\r\n          />\r\n        </td>\r\n        <td>{item.ref}</td>\r\n        <td>€{item.price}</td>\r\n        <td>\r\n          <div className=\"btn-group\" role=\"group\" aria-label=\"Basic example\">\r\n            <button\r\n              type=\"button\"\r\n              className=\"btn btn-secondary\"\r\n              onClick={() => {\r\n                if (qty > 1) {update('decrement') }\r\n              }}\r\n              >\r\n              -\r\n            </button>\r\n            <span className=\"btn btn-light\">{qty}</span>\r\n            <button\r\n              type=\"button\"\r\n              className=\"btn btn-secondary\"\r\n              onClick={() =>  {update('increment')}}\r\n              >\r\n              +\r\n            </button>\r\n          </div>\r\n        </td>\r\n        <td>€{(quantity *  item.price).toFixed(2)}</td>\r\n        <td>\r\n          <button\r\n            type=\"button\"\r\n            className=\"btn btn-danger remove\"\r\n             onClick={() => remove(id)}>\r\n            X\r\n          </button>\r\n        </td>\r\n      </tr>\r\n    );\r\n}\r\n\r\nconst Table = ({ items }) => {\r\n    return (\r\n      <table>\r\n        <tr>\r\n          <th width=\"200\">Produit</th>\r\n          <th width=\"80\">Référence</th>\r\n          <th width=\"150\">Prix</th>\r\n          <th width=\"150\">Quantité</th>\r\n          <th width=\"200\">Total</th>\r\n        </tr>\r\n        {items.map(item => {\r\n          return(<Row item={item}/>)\r\n        })}\r\n      </table>\r\n    );\r\n  }\r\n\r\nexport const CartPage = () => {\r\n    const items = useSelector(state => state.items)\r\n    const [subTotal, setSubTotal] = useState(0.00)\r\n    const [total, setTotal] = useState(0.00)\r\n    const shipping = 0.00\r\n\r\n    useEffect(() => {\r\n      let totals = items.map(item => {\r\n        return item.quantity * item.details.price\r\n      })\r\n      setSubTotal(totals.reduce((item1, item2) => item1 + item2, 0))\r\n      setTotal(subTotal + shipping)\r\n      console.log(`Subtotal:  €${subTotal} `)\r\n      console.log(`You have ${items.length} in your cart`)\r\n    }, [items, subTotal, total])\r\n    return (\r\n      <Fragment>\r\n        <div className=\"container\">\r\n        <div className=\"row\">\r\n          <div className=\"col-sm cart\">\r\n              <Table items={items}/>\r\n          </div>\r\n\r\n          <div className=\"col-sm-3 order-summary\">\r\n            <ul className=\"list-group\">\r\n             \r\n\r\n              <li className=\"list-group-item\">\r\n              \r\n              <li className=\"list-group-item \">\r\n                <ul className=\"list-group flex\">\r\n                  <li className=\"text-left\">Valeur Commande</li>\r\n                    <li className=\"text-right\">€{subTotal == 0.00 ? \"0.00\" : total.toFixed(2)}</li>\r\n                </ul>\r\n              </li>\r\n                <ul className=\"list-group flex\">\r\n                  <li className=\"coupon crimson\">\r\n                    <small> Ajouter code promo</small>\r\n                  </li>\r\n                </ul>\r\n              </li>\r\n\r\n              \r\n            </ul>\r\n              <Link to=\"checkout\" className={`white btn btn-light btn-lg btn-block checkout ${!items.length && 'disabled'} bg-crimson`}>\r\n                Valider\r\n              </Link>\r\n          </div>\r\n        </div>\r\n        </div>\r\n      </Fragment>\r\n    );\r\n}","C:\\Users\\Grégoire Antoine\\Documents\\GitHub\\DEVWEB3\\fronte